{
  "Logging": {
    "IncludeScopes":  false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console":  {
      "LogLevel":  {
        "Default":  "Warning",
        "Jaeger.Reporter.LoggingReporter":  "Information",
        "HelloApi.TracingMiddleware":  "Information"
      }
    },
    "LoggingReporter":  {
      "LogLevel":  {
        "Default":  "Warning"
      }
    }
  },
  "AllowedHosts": "*"
}
